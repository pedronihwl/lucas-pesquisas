{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        "dist/**",
        "!dist/**/*.map"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
